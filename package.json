    "cors": "^2.8.5",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "find-node-modules": "^2.1.3",
    "morgan": "^1.10.0",
    "open": "^8.4.0",
    "ramda": "^0.28.0"
  }
